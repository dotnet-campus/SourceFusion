<Project>

  <PropertyGroup Condition=" $(IsInDemoToolDebugMode) == 'True' ">
    <NuGetWalterlvToolPath>$(MSBuildThisFileDirectory)..\..\bin\$(Configuration)\netcoreapp2.0\codet.dll</NuGetWalterlvToolPath>
  </PropertyGroup>

  <PropertyGroup Condition=" $(IsInDemoToolDebugMode) != 'True' ">
    <NuGetWalterlvToolPath>$(MSBuildThisFileDirectory)..\tools\netcoreapp2.0\codet.dll</NuGetWalterlvToolPath>
  </PropertyGroup>

  <PropertyGroup>
    <CvteCompilerGeneratedCodeFolder>obj\$(Configuration)\$(TargetFramework)\Cvte.Compiler.GeneratedCodes</CvteCompilerGeneratedCodeFolder>
  </PropertyGroup>

  <Target Name="CvteCompilerGenerateCode" BeforeTargets="CoreCompile">
    <Exec Command="dotnet &quot;$(NuGetWalterlvToolPath)&quot; -p &quot;$(MSBuildProjectDirectory)&quot; -i $(CvteCompilerGeneratedCodeFolder) -c &quot;@(Compile)&quot;" />
  </Target>

  <Target Name="_IncludeCvteCompilerGeneratedCode" DependsOnTargets="CvteCompilerGenerateCode">
    <ItemGroup>
      <Compile Include="$(CvteCompilerGeneratedCodeFolder)\*.cs" />
    </ItemGroup>
  </Target>

</Project>